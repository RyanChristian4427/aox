SubDir TOP cryptlib crypt ;

CIPHERS =
    aescrypt.c aeskey.c aestab.c bfecb.c bfskey.c castecb.c castskey.c
    descbc.c desecb3.c desecb.c desskey.c icbc.c iecb.c iskey.c md2dgst.c
    md4dgst.c md5dgst.c rc2cbc.c rc2ecb.c rc2skey.c rc4skey.c rc5ecb.c
    rc5skey.c rmddgst.c sha1dgst.c sha2.c skipjack.c aes_modes.c ;

if $(USE_X86_ASM) {
    CIPHERS += b-elf.s c-elf.s d-elf.s m5-elf.s r4-elf.s r5-elf.s
               rm-elf.s s1-elf.s ;
}
else {
    CIPHERS += bfenc.c castenc.c desenc.c rc4enc.c rc5enc.c ;
}

Build cryptlib : $(CIPHERS) ;
